{
  "manifest_version": 2,
  "name": "Octoview",
  "version": "1.0.0",
  "description": "The missing preview feature for GitHub",
  "homepage_url": "https://github.com/pd4d10/octoview",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": ["dist/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["dist/contentscript.js"],
      "run-at": "document_idle"
    }
  ]
}
